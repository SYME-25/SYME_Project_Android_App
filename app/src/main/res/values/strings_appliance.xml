    <string name="home_appliance_desc_refrigerator">Keeps food and beverages cold to preserve freshness.</string>
    <string name="home_appliance_desc_freezer">Stores food at very low temperatures for long-term preservation.</string>
    <string name="home_appliance_desc_washing_machine">Cleans clothes automatically using water and electricity.</string>
